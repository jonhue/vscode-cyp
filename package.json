{
    "name": "vscode-cyp",
    "displayName": "cyp",
    "description": "cyp language package",
    "version": "1.4.0",
    "engines": {
        "vscode": "*"
    },
    "publisher": "jonhue",
    "repository": {
        "type": "git",
        "url": "https://github.com/jonhue/vscode-cyp.git"
    },
    "license": "MIT",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cyp",
                "aliases": [
                    "cyp"
                ],
                "extensions": [
                    ".cprf",
                    ".cthy"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cyp",
                "scopeName": "source.cyp",
                "path": "./syntaxes/cyp.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "ovsx": "^0.5.1",
        "vsce": "^2.11.0"
    }
}
